    "compile:prod": "npm run compile",
    "compile:dev": "cross-env ENV=dev webpack --config src/config/webpack.config.dev.js",
    "compile:server": "babel src -d compiled --ignore spec.js,test.js",
    "------ SERVER STEPS ------": "",
    "start": "node compiled/server-entry.js",
    "start:webpack": "cross-env ENV=dev webpack-dev-server --config webpack-dev-server.js",
    "start:dev": "cross-env ENV=dev nodemon src/server-entry.js",
    "start:cc": "concurrently \"npm run start:webpack\" \"npm run start:dev\" ",
    "------ TESTING ------": "",
    "test": "jest --config jest.json .*.spec.jsx?",
    "------ UTILS ------": "",
    "nuke": "rimraf node_modules && yarn && yarn test"
